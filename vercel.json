{
  "version": 2,
  "outputDirectory": "apps/running-game-react/build",
  "builds": [
    {
      "src": "apps/running-game-react/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "cd apps/running-game-react && npm install && npm run build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "installCommand": "npm install"
}
