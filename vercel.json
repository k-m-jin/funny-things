{
  "version": 2,
  "builds": [
    {
      "src": "apps/running-game-react/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/apps/running-game-react/index.html" }
  ],
  "installCommand": "npm install"
}
